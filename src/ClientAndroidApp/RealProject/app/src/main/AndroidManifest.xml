<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.realproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RealProject"
        android:usesCleartextTraffic="true">
        <activity android:name=".EditItem"></activity>
        <activity android:name=".ItemInfo" />
        <activity android:name=".RegisterScreen" />
        <activity android:name=".LoadingPage" />
        <activity android:name=".MainMenu" />
        <activity android:name=".AddScreen" />
        <activity android:name=".Items" />
        <activity android:name=".LoginPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>